{
  "manifest_version": 3,
  "name": "Youtube Downloader",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["*://www.youtube.com/*", "*://youtube.com/*"],
    "js": ["contentScript.js"]
  }],
  "permissions": [
    "webNavigation",
    "tabs"
  ]
}